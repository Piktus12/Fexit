{# templates/default/new.html.twig #}
{%  extends 'base.html.twig' %}

{% block title %}FeXiT{% endblock %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.FirstName) }}
    {{ form_row(form.LastName) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
